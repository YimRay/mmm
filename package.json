{
  "name": "mmm",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "candy-script start",
    "build": "candy-script build",
    "deploy": "gh-pages -d dist",
    "analyzer": "candy-script analyzer",
    "commit": "git-cz && git push",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:rc": "standard-version --prerelease rc",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch"
  },
  "keywords": [],
  "author": "吴胤瑞",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@laconic/react-script": "^1.2.0",
    "@types/node": "^20.2.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "commitizen": "^4.3.0",
    "cz-git": "^1.6.1",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.5",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "homepage": "http://xun082.github.io/mmm"
}